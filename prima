package no2;

import java.util.Scanner;

/**
 *
 * @author Praktikum
 */
public class No2 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner in= new Scanner (System.in);
        int a,akh,bil;
        System.out.print("Awal : "); a=in.nextInt();
        System.out.print("Akhir : "); akh=in.nextInt();
        System.out.printf("Bilangan prima dari %d sampai %d adalah : ",a,akh);
        int angka=a;
        while(angka <= akh){
            int tmp =0;
            for(int bagi = 2; bagi < angka; bagi++) {
                if(angka%bagi==0) 
                   tmp=1;
            }
        }
         if(tmp!=1) {
         System.out.println(angka);
            angka++;
            
    }
    }
        
    
    
}
        
